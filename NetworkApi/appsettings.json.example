{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning",
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning",
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },

  "System": {
    "ProxyLevel": 1,
    "InstanceName": "kerby"
  },

  "Cache" : {
    "Enabled": true,
    "Type": "Distributed",
    "Workers": 4,
    "Interval": 1000
  },

  "Database": {
    "MongoDB": {
      "DatabaseName": "Sensate",
      "ConnectionString": "mongodb://sensate:mongopass@127.0.0.1:27017/Sensate",
      "MaxConnections": 300
    },
    "PgSQL": {
      "ConnectionString": "User ID = postgres;Password=DefaultPassword;Server=127.0.0.1;Port=5432;Database=Sensate"
    },
    "Redis": {
      "Host": "127.0.0.1",
      "InstanceName": "Sensate"
    }
  },

  "PublicUrl": "http://localhost:5000", 

  "Authentication" : {
    "JwtRefreshExpireMinutes": 43200,
    "JwtKey": "<key>",
    "JwtIssuer": "http://localhost:5000/",
    "JwtExpireMinutes": 43200,
    "PrimaryAuthHost": false,
	  "Scheme": "https"
  },

  "Mqtt": {
    "PublicBroker": {
      "Username": "",
      "Password": "",
      "Ssl": "false",
      "Port": 1883,
      "Host": "",
      "RealTimeShareTopic": "sensate/measurements/rt",
      "ShareTopic": "sensate/measurements",
      "BulkShareTopic": "sensate/measurements/bulk",
      "ActuatorTopic": "sensate/actuator/$sensorId"
    },
    "InternalBroker": {
      "Username": "",
      "Password": "",
      "Ssl": "false",
      "Port": 1883,
      "Host": "",
      "InternalMeasurementTopic": "sensate/internal/measurements",
      "InternalBulkMeasurementTopic": "sensate/internal/measurements/bulk"
    } 
  }
}