{
  "name": "live-data-service",
  "version": "1.1.0",
  "description": "LiveDataService",
  "main": "dist/src/live-data-service.js",
  "scripts": {
    "start": "node dist/src/app/live-data-service.js",
    "build": "tsc -t ES2018 --noUnusedLocals --noImplicitReturns",
    "generate-js": "node node_modules/protobufjs/bin/pbjs -t static-module -w commonjs -o generated/proto.js ../SensateIoT.Platform.Network.Contracts/DTO/Measurement.proto ../SensateIoT.Platform.Network.Contracts/DTO/Message.proto",
    "generate-ts": " node node_modules/protobufjs/bin/pbts -o generated/proto.d.ts generated/proto.js",
    "dev": "nodemon -x \"node ./node_modules/ts-node/dist/bin.js\" ./src/app/live-data-service.ts",
    "generate-js-docker": "node node_modules/protobufjs/bin/pbjs -t static-module -w commonjs -o generated/proto.js Contracts/DTO/Measurement.proto Contracts/DTO/Message.proto"
  },
  "author": {
    "name": "Michel Megens",
    "email": "michel@michelmegens.net"
  },
  "license": "GPL-3.0",
  "repository": "https://github.com/sensate-iot/SensateService",
  "devDependencies": {
    "@types/async": "^2.4.2",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.27",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^8.10.64",
    "@types/pg": "^7.14.5",
    "@types/ws": "^6.0.4",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "async": "^2.6.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.0",
    "mongoose": "^5.10.7",
    "mqtt": "^3.0.0",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "pg-pool": "^2.0.10",
    "protobufjs": "^6.10.2",
    "ws": "^6.2.1",
    "zlib": "^1.0.5"
  }
}
