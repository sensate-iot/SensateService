{
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    }
  },

  "Gateway": {
    "Messages": "http://localhost:6500/network/v1/gateway/messages",
    "Measurements": "http://localhost:6500/network/v1/gateway/measurements"
  },

  "Database": {
    "MongoDB": {
      "DatabaseName": "Sensate",
      "ConnectionString": "mongodb://root:root@localhost:27017/Sensate?authSource=admin",
      "MaxConnections": 300
    }
  },

  "Mqtt": {
    "Username": "sensate",
    "Password": "sensateiot",
    "Ssl": "false",
    "Port": 1883,
    "Host": "127.0.0.1",
    "MeasurementTopic": "$share/sensate/sensate/measurements",
    "BulkMeasurementTopic": "$share/sensate/sensate/measurements/bulk",
    "MessageTopic": "$share/sensate/sensate/messages",
    "BulkMessageTopic": "$share/sensate/sensate/messages/bulk"
  }
}
